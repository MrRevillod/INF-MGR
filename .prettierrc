{
    "singleQuote": false,
    "semi": false,
    "useTabs": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "printWidth": 80,
    "endOfLine": "lf",
    "arrowParens": "avoid",
    "plugins": [
        "prettier-plugin-svelte"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte",
                "useTabs": true,
                "tabWidth": 4
            }
        }
    ]
}